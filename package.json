{
    "name": "vmcode",
    "displayName": "Vasm Lang",
    "description": "Syntax highlighting for vasm lang.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "vasm",
                "aliases": [
                    "vasm",
                    "Vasm",
                    "VASM"
                ],
                "extensions": [
                    ".vasm",
                    ".hasm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vasm",
                "scopeName": "source.vasm",
                "path": "./syntaxes/vasm.tmLanguage.json"
            }
        ]
    }
}
